
{% for type in types.all|!protocol|!enum %}

extension {{ type.name }} {

    init({% for var in type.storedVariables %}{{ var.name }}: {{ var.typeName }}{% if not forloop.last %},{% endif %}{% endfor %}) {
    {% for var in type.storedVariables %}
        self.{{ var.name }} = {{ var.name }}
    {% endfor %}
    }
}
{% endfor %}

